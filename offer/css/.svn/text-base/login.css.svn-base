body,
div,
dl,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
pre,
code,
form,
fieldset,
legend,
p,
blockquote,
th,
td,
img {
  margin: 0;
  padding: 0;
  border: 0;
  outline: none;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  -webkit-user-select: none;
}
input,
button,
textarea {
  margin: 0;
  padding: 0;
  border: 0;
  outline: none;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
input:-moz-placeholder {
  color: #a5a5a5;
}
::-webkit-input-placeholder {
  color: #a5a5a5;
}
input {
  -webkit-appearance: none;
  -webkit-border-radius: 0;
  outline: none;
}
ul,
ol,
li {
  list-style: none;
}
a {
  color: #333;
  text-decoration: none;
  -webkit-touch-callout: none;
  cursor: pointer;
  outline: none;
}
a:visited {
  color: #333;
}
a:hover {
  color: #333;
  text-decoration: none;
}
a:active {
  color: #333;
}
em,
i {
  font-style: normal;
}
img {
  border: none;
  height: auto;
  max-width: 100%;
}
.fl {
  float: left;
  display: inline;
}
.fr {
  float: right;
  display: inline;
}
.clear {
  height: 0px;
  line-height: 0px;
  clear: both;
}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
* + html .clearfix {
  overflow: hidden;
}
* html .clearfix {
  height: 1%;
}
.login_main {
  width: 100%;
}
.login_main .login_logo {
  width: 8.16rem;
  height: 2.4rem;
  margin: 1.06666667rem auto;
}
.login_logo img {
  display: block;
  width: 100%;
  height: 100%;
}
.form_body {
  width: 10rem;
  margin: 0 auto;
}
.form_body .form_main {
  padding-left: 0.4rem;
}
.form_body .form_main .cell {
  border-bottom: 1px solid #d8d8d8;
}
.cell {
  position: relative;
}
.cell .cell_text {
  float: left;
  font-size: 15px;
  color: #000;
  padding: 0.4rem 0;
  width: 1.46666667rem;
}
[data-dpr="2"] .cell .cell_text {
  font-size: 30px;
}
[data-dpr="2.5"] .cell .cell_text {
  font-size: 38px;
}
[data-dpr="2.75"] .cell .cell_text {
  font-size: 41px;
}
[data-dpr="3"] .cell .cell_text {
  font-size: 45px;
}
[data-dpr="4"] .cell .cell_text {
  font-size: 60px;
}
.cell .cell_input {
  float: left;
  color: #000;
  font-size: 15px;
  padding: 0.4rem 0 0.4rem 1.06666667rem;
}
[data-dpr="2"] .cell .cell_input {
  font-size: 30px;
}
[data-dpr="2.5"] .cell .cell_input {
  font-size: 38px;
}
[data-dpr="2.75"] .cell .cell_input {
  font-size: 41px;
}
[data-dpr="3"] .cell .cell_input {
  font-size: 45px;
}
[data-dpr="4"] .cell .cell_input {
  font-size: 60px;
}
.cell .clear_btn {
  position: absolute;
  width: 0.6rem;
  height: 0.6rem;
  background: url(../images/clear.png) no-repeat;
  background-size: 100% 100%;
  top: 0.26666667rem;
  right: 0.45333333rem;
  display: none;
}
.cell .check_pos {
  position: absolute;
  top: 0.24rem;
  right: 0.45333333rem;
}
.forget a {
