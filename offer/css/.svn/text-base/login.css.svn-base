.login_no .register {
  color: #576b95;
  font-size: 14px;
  display: inline-block;
}
[data-dpr="2"] .login_no .register {
  font-size: 28px;
}
[data-dpr="2.5"] .login_no .register {
  font-size: 35px;
}
[data-dpr="2.75"] .login_no .register {
