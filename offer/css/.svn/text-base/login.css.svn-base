}
[data-dpr="2"] .login_btn a {
  font-size: 28px;
}
[data-dpr="2.5"] .login_btn a {
  font-size: 35px;
}
[data-dpr="2.75"] .login_btn a {
  font-size: 39px;
}
[data-dpr="3"] .login_btn a {
