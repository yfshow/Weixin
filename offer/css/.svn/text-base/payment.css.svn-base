.fr{float: right;}
.fl{float: left;}
.pay-OK{padding:1.5rem 0;background: #fff}
.pay-OK img{display: block;margin: 3rem auto;width: 7.2rem;height:7.2rem;}
.pay-OK span{font-size: 1.5rem;color: #333;text-align: center;margin-top: 2rem;display: block;margin-bottom: 1rem;}

.pay-btnB{overflow: hidden;  width:100%;    text-align: center;margin: 1.5rem 0;}
.pay-btnB a{display: inline-block;padding:0.5rem 1rem;font-size: 1.5rem;border: 1px solid #999;border-radius: 0.5rem;background: #fff;box-sizing:border-box; color: #666;margin:0 0.5rem;}
.pay-btnB .btn-green{border-color:#149818;background: #1cbe20;color: #fff;}
.prompt-box{padding: 1rem;font-size: 1.1rem;color: #666}


.pay-OK .pay-details{text-align: center;color: #666;text-align: left;width:17.5rem;margin:0 auto;font-size: 1.3rem;}
.pay-OK .pay-details li{margin: 0.5rem 0;}

.move-BOX h6{padding:0 1rem;color: #666;font-size:1.4rem;font-weight: 100;margin-bottom: 0.5rem}
.move-BOX .passed-box{background: #fff;font-size: 1.4rem;padding: 0 1rem;height:5rem;line-height: 5rem;}
.move-BOX .passed-box em{color: #F24E4A}

.pay-product .full-ul{margin-top: 0}
.pay-product .full-ul li:last-child{text-align: left; padding: 0.75rem 1rem; }
.full-wb .go-payment{ display: inline-block;width:7.5rem;text-align: center;font-size: 1.4rem;border: 1px solid #999;border-radius: 0.3rem; color: #00c000;border-color: #00c000;}
.pay-frame{overflow: hidden;word-wrap:break-word;}
.pay-frame .go-payment{float: right;}
.full-wb{display: none;}
.full-wb ul:last-child{margin-bottom: 0;border-bottom: 0}
.open{margin-bottom: 1rem;background: #fff;border-top: 1px solid #d8d8d8;}

.d-Tit2{ height:4rem;line-height: 4rem;color: #333;font-size: 1.5rem;font-weight: 100;text-align: center;background: #fff;border-bottom:1px solid #d8d8d8;}
.d-Tit6{height: 9.75rem;line-height: 9.75rem;font-size: 3.6rem;text-align: center;background: #fff;border-bottom:1px solid #d8d8d8;}
.d-Tit6 em{font-size:2.5rem;}

.full-by {
    border-top: 1px solid #d8d8d8;
    border-bottom: 1px solid #d8d8d8;
    margin: 10px auto;
    background-color: #fff;
    font-size: 1.4rem;
    overflow: hidden;
    padding: 1.25rem 1rem;
}
.full-by .gray{color: #999;}
.sub-btn {
    width: 90%;
    height: 4.4rem;
    line-height: 4.4rem;
    text-align: center;
    border-radius: 5px;
    background-color: #04be02;
    margin: 0 auto;
    font-size: 1.6rem;
    color: #fff;
    margin-top: 2rem;
}