body,p,ul,ol,li,dl,dt,dd,h1,h2,h3,h4,h5,h6,form,fieldset,legend,input,select,textarea,button,th,td,blockquote,address,pre{margin:0;padding:0;}
h1,h2,h3,h4,h5,h6,input,textarea,select,button,label{font-size:100%;vertical-align:middle;}
ul,dl,ol{list-style:none;}
img,fieldset{border:none;}
img{display:inline-block;overflow:hidden;vertical-align:top;}
em,address{font-style:normal;}
sup,sub{vertical-align:baseline;}
table{border-collapse:collapse;border-spacing:0;}
q:before{content:"";}
q:after{content:"";}
button{cursor:pointer;}
textarea{word-wrap:break-word;resize:none;}
a,button,input,textarea{-webkit-tap-highlight-color: rgba(0,0,0,0;)-webkit-user-modify:read-write-plaintext-only;-webkit-appearance:none;}

*{margin:0;padding:0;}
body{width:7.5rem;background-color: #efeff4;margin:0 auto;color:#333;font-family: "STHeiti","Microsoft YaHei",Helvetica,Arial,sans-serif;}
.main{width:7.5rem;margin:0 auto;}
ul.tab{height: 0.9rem;width:100%;background-color:#fff;}
/*.wrapContent{background-color:#fff;}*/
ul.tab li{width:33.33%;display: inline-block;float:left;text-align: center;height: 0.9rem;}
ul.tab li p{margin:0 0.25rem;}
ul.tab li span{border-bottom: 0.04rem solid #fff;font-size:0.3rem;height:0.86rem;line-height: 0.86rem;display:block;}
ul.tab li.active span{color:#04be02;border-bottom: 0.04rem solid #04be02;}

.timeBan{height:0.7rem;line-height:0.7rem;background-color: #efeff4;border-top:1px solid #d8d8d8;border-bottom:1px solid #d8d8d8;position: relative;}
.timeBan span{font-size:0.3rem;float:left;padding-left: 0.2rem;}
.timeBan .down{width:1.6rem;padding-right:0.2rem;float:right;} 
.timeBan .down b{color:#ee4120;background:url("../images/down.png") no-repeat right center;padding-right:0.3rem;font-size:0.28rem;display: block;    background-size: 0.25rem 0.13rem;font-weight: normal;}

.wrap{display: none;}
.content{padding:0 0.25rem;height:1.4rem;border-bottom: 1px solid #d8d8d8;background-color: #fff;}
.content .part1{width:1.8rem;float:left;}
.part1 h2{font-size:0.3rem;color:#999;font-weight: normal;padding:0.28rem 0 0.15rem;}            
.part1 p{font-size:0.2rem;color:#999;overflow: hidden;text-overflow:ellipsis;white-space: nowrap;}

.content .part2{width:3rem;height:1.4rem;padding-left:0.8rem;display: block;float:left;margin-left:0.1rem;}
.part2.icon1{background:url("../images/text.png") no-repeat left center; background-size: 0.72rem 0.72rem;}
.part2.icon2{background:url("../images/cash.png") no-repeat left center; background-size: 0.72rem 0.72rem;}
.part2.icon3{background:url("../images/return.png") no-repeat left center; background-size: 0.72rem 0.72rem;}
.part2 h2{font-size:0.33rem;font-weight: normal;padding:0.28rem 0 0.05rem;}            
.part2 p{font-size:0.28rem;color:#666;overflow: hidden;text-overflow:ellipsis;white-space: nowrap;}

.content .part3{width:1rem;float:left;}
.part3 h2{font-size:0.3rem;color:#999;font-weight: normal;padding:0.28rem 0 0.15rem;} 
.part3 .green{color:#04be02;}

.content .part4{width:0.13rem;height:0.25rem;float:right;}
.part4 img{width:0.13rem;height:0.25rem;padding-top:0.55rem;}        
#picktime{opacity: 0;position: absolute;height: 0.7rem;width: 1.6rem;top:0;right: 0.2rem;}    
footer{height:1.65rem;background-color: #EFEFF4;line-height: 1.65rem;font-size:0.24rem;color:#999;text-align: center;}

.contentWrap{border-bottom: 1px solid #d8d8d8;border-top:1px solid #d8d8d8;margin-bottom: 0.2rem;background-color: #fff;}
.title{height:0.8rem;line-height: 0.8rem;text-align: center;font-size:0.3rem;border-bottom: 1px solid #d8d8d8;}
.resultShow{text-align: center;padding:0.5rem 0;}
.resultShow h2{font-size:0.7rem;}
.resultShow p{font-size:0.28rem;color:#999;}
.resultInfo{border-top: 1px solid #d8d8d8;margin:0 0.3rem;height:0.9rem;line-height: 0.9rem;}
.resultInfo span{float:left;font-size:0.3rem;color:#999;width:1.4rem;text-align: left;display: block;}
.resultInfo b{float:right;text-align: right;font-size:0.3rem;width:5.3rem;display: block;font-weight: normal;}
.resultInfo .infor{float:right;text-align: right;font-size:0.3rem;width:5.3rem;word-wrap: break-word;word-break:break-all;line-height: 0.4rem;}
.gray{background-color: #efeff4;}
.noTopBorder{border-top:none;}
.inforSpecial{height: auto;padding:0.25rem 0;overflow: hidden;}
.inforSpecial span{line-height:0.4rem;}


.inforList{font-size:0.3rem;}
.inforList i{color:#f24e4a;font-style: normal;}
.resultInfo .left{float:left;font-size:0.3rem;width:2.13rem;}
.conWrap .left{width:2.3rem;}
.resultInfo .center{float:left;font-size:0.3rem;width:4.4rem;}
.resultInfo .right{float:right;width:0.35rem;height:0.9rem;text-align: right;}
.resultInfo .right img{width:0.13rem;height:0.25rem;padding-top:0.3rem;}
.resultInfo .center input{height:0.9rem;line-height: 0.9rem;border:none;width:4.7rem;display: block;}
input::-webkit-input-placeholder{color:#ccc;}
p.remind{font-size:0.26rem;color:#888;height:0.8rem;line-height: 0.8rem;text-align: center;}
.nextBtn{width:7.12rem;margin:0 auto 0.4rem;height:0.92rem;}
.nextBtn span{background-color: #d8d8d8;color:#fff;line-height: 0.9rem;height:0.9rem;width:7.1rem;display: block;border-radius:0.1rem;font-size:0.34rem;border:1px solid #d8d8d8;}
.nextBtn span.active{background-color: #04be02;border:1px solid #039802;text-align:center;}

.remindText{height:1rem;font-size:0.26rem;line-height: 1rem;color:#666;padding-left:0.3rem;}
p.text{font-size:0.26rem;color:#666;padding-top:0.1rem;padding-left:0.3rem;line-height:0.8rem;}
.inputGroup{border: 1px solid #999;height:0.88rem;margin-left:0.3rem;width:5.29rem;margin-bottom: 0.6rem;}
.inputGroup input {border:none;border-left: 1px solid #e8e8e6;display: inline-block;float: left;font-size: 0.26rem;height: 0.88rem;line-height: 0.88rem;margin-left: -1px;text-align: center;width: 0.88rem;}
.inputGroup input:nth-child(1){border-left:none;margin-left:0;}
.forget{text-align: right;height:0.6rem;padding:0.2rem 0.3rem 0.2rem 0;}
.forget span{font-size:0.24rem;color:#576b95;text-decoration: underline;height:0.6rem;line-height: 0.6rem;display: block;}

.noRecords{text-align: center;font-size:0.3rem;color:#666;padding:4rem 0;background-color: #efeff4;}
.remindTit{padding:0.3rem;}
.remindTit p{line-height:0.42rem;height:0.42rem;font-size:0.28rem;color:#666;}

.wrapContent .ok{width:1.84rem;margin:0 auto;}
.wrapContent .ok img{width:1.84rem;height:1.84rem;padding:1.3rem 0 0.25rem;} 
p.infor{line-height:0.48rem;font-size:0.26rem;color:#666;padding:0 0.3rem 0.8rem;} 
p.takeCare{font-size:0.24rem;color:#999;line-height:0.48rem;padding:0.2rem 0.4rem;}

.viewDetail{width:7.12rem;margin:0 auto;height:0.92rem;padding-bottom:0.4rem;}
.viewDetail button{background-color: #fff;color:#04be02;line-height: 0.9rem;height:0.9rem;width:7.1rem;display: block;border-radius:0.1rem;font-size:0.34rem;border:1px solid #039802;}

 /*shop-record*/
.wrapper{width: 100%; overflow: hidden;background: #eee;height: 100%;position: fixed;}
.wrap-box{position:relative; width: 100%; -webkit-transition:-webkit-transform 0.5s ease-out 0s; z-index: 9; }
#scroller-pullDown{height: 50px;  line-height:30px; padding: 10px 0; text-align:center; font-size: 14px; width: 100%;  position:absolute; left: 0; top: -50px;  }
#scroller-pullDown.on{height: 50px;  line-height:30px; padding: 10px 0; text-align:center; font-size: 14px; width: 100%;  position:relative; left: 0; top: 0;  }
#scroller-pullUp{height: 50px;  line-height:50px;  text-align:center; font-size: 14px; width: 100%;  position:absolute; left: 0; bottom: -50px; }

/*------------------输入密码 shx 1027 -------------------*/
.pwd-box{
width:270px;margin-left: 0.3rem;margin-bottom: 0.6rem;
padding-left: 1px;
position: relative;
border: 1px solid #9f9fa0;
}
.pwd-box input[type="tel"]{
width: 100%;
height: 48px;
color: transparent;
position: absolute;
top: 0;
left: 0;
border: none;
font-size: 18px;
opacity: 0;
z-index: 1;
letter-spacing: 35px;
}
.fake-box input{
width: 44px;
height: 48px;line-height: 48px;
border: none;
border-right: 1px solid #e5e5e5;
text-align: center;
font-size: 30px;
display: inline;float: left;border-radius: 0;
}
.fake-box input:nth-last-child(1){
border:none;
}

.pwd-box .pwd-input:focus{ 
left:-1000px;
top: -100px;
}
.fake-box{font-size: 12px;height: 48px;}