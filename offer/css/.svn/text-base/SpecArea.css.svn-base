.Area-goods img{width: 6.5rem;height: 8.5rem;margin-top: 0.8rem;}
.goods-title{padding: 0 0.9rem;font-size: 1.2rem;}
.goods-piece{padding: 0 0.9rem;font-size: 1.4rem;color: #f24e4a;font-weight: bold;}
.Area-bottom{height:5rem;line-height: 5rem;border-top: 1px solid #d8d8d8;background-color: #fff;color: #000;font-size: 1.7rem;position: fixed;bottom: 0;width: 100%;}
.Area-bottom div{height: 5rem;width: 33.3%;border-right: 1px solid #d8d8d8;float: left;text-align: center;}
.Area-bottom div:last-child{border-right: 0px;}
.Area-bottom div span{background-color: #f24e4a;color: #fff;font-size: 1.2rem;font-weight: normal;padding: 0 0.5rem;border-radius: 1.5rem;position: relative;top:-1rem;line-height: 1.2rem;}

/*--------------------管理收货地址 yf--------------------*/
.ads-list{background-color: #fff;margin-bottom:1rem;padding-top: 1rem;border-top: 1px solid #d8d8d8;border-bottom: 1px solid #d8d8d8}
.ads-title{font-size: 1.6rem;color: #333;padding: 0 1rem;}
.ads-title .tel{font-size: 1.4rem; margin-left: 1rem;}
.ads-title .default{font-size: 1.4rem;color: #00c000;float: right;margin-right: 1rem;}
.ads-detial{font-size: 1.4rem;color: #707070;padding: 1rem;}
.ads-contral{font-size: 1.4rem;text-align: right;border-top: 1px dotted #ececec;padding: 0 1rem;color: #707070;line-height: 4rem;}
.ads-contral span{border: 1px solid #999;margin-left: 2rem;padding: .4rem .4rem .4rem 2.5rem;background-repeat: no-repeat;background-size: 1.4rem;background-position: 15% 50%;border-radius: 3px;}
.ads-contral .edit{ background-image: url(../images/icon-edit.png);}
.ads-contral .detele{background-image: url(../images/icon-delete.png);}
.ads-contral .setDefault{float: left;background-image: url(../images/icon-noselected.png);padding-left: 2rem;background-repeat: no-repeat;background-size: 1.5rem;background-position: 0 50%;}
.ads-contral .setDefault.on{background-image: url(../images/icon-selected.png);}
.bottom-btn{width: 100%;height: 5rem;line-height: 5rem;background-color: #56ba22;color: #fff;text-align: center;font-size: 1.6rem;position: fixed;bottom: 0px;}
.noads{text-align: center;color: #666666;font-size: 1.7rem;line-height: 4rem;padding-top: 13rem;display: none;}
.noads img{width: 7.5rem}

/*--------------------新建联系人 yf--------------------*/
.new-ads{background-color: #fff;padding-left:1rem;}
.new-ads li{font-size: 1.5rem;line-height: 5rem;border-bottom: 1px solid #d8d8d8}
.new-ads li:last-child{border-bottom: 0px;padding-right: 1rem;}
.new-ads li label{display: block;float: left;}
.new-ads li input{font-size: 1.4rem;line-height: 5rem;}
.new-ads li select{font-size: 1.4rem;width: 7.5rem;}
.new-ads li textarea{background-color: #fff;width: 100%;height: 7rem;border-radius: 5px;padding:0.5rem;box-sizing:border-box;font-size: 1.3rem;border:1px solid #d8d8d8;color: #666;-webkit-appearance: none;}

/*专营区商品列表页*/
.search{width: 100%;height: 6rem;padding-top: 1.5rem;background-color: #f8f8f8;}
.search-bar{width: 86%;height: 3rem;line-height: 3rem;background-image: url(../images/area-search.png);background-repeat: no-repeat;background-size: 1.5rem;background-position: 1rem 50%;padding-left: 3rem;background-color: #e5e5e5;margin-left: 7%;border-radius: 5px;}
.search-bar input{background-color: #e5e5e5;font-size: 1.3rem;min-width: 78%;}
.search-bar span{background-color: #00c000;padding: .5rem;color: #fff;float: right;line-height: 2rem;width: 4rem;text-align: center;border-bottom-right-radius: 5px;border-top-right-radius: 5px;font-size: 1.3rem;}

.shop{min-height: 6rem;width: 100%;background-color: #fff;padding: 0.5rem 1rem;border-bottom: 1px solid #d8d8d8;margin-bottom: 1rem;}
.shop-title{font-size: 1.5rem;line-height: 3rem}
.shop-text{font-size: 1.2rem;color: #666;line-height: 2rem}


.classify{min-height: 9rem;width: 100%;background-color: #fff;border-bottom: 1px solid #d8d8d8;margin-bottom: 1rem;}
.classify-title{color:#666;font-size: 1.4rem;line-height: 4.5rem;background-image: url(../images/fire.png);background-repeat: no-repeat;background-size: 1.2rem;background-position: 1rem 50%;border-bottom: 1px solid #d8d8d8;padding: 0 1rem 0 2.6rem;}
.classify-text{font-size: 1.2rem;color: #666;padding: 0 1rem 1rem 1rem;}
.classify-text span{padding: 0.5rem 1rem;background-color: #f4f4f4;border-radius: 5px;display: inline-block;margin: 1rem 1rem 0 0;}


.sort-bar{height: 4.5rem;width: 100%;line-height: 4.5rem;border-top: 1px solid #d8d8d8;background-color: #fff;border-bottom: 1px solid #d8d8d8;}
.sort-bar div{width: 33.3%;text-align: center;font-size: 1.4rem;float: left;}
.sort-bar div.on{color: #00c000}
.sort-bar div.sift-normal:after{content: "";width: 1rem;height: 1rem;background-image: url(../images/sift-normal.png);background-size: auto 100%;background-repeat: no-repeat;background-position: 100% 100%;display: inline-block;}
.sort-bar div.sift-normal.sift-up:after{background-image: url(../images/sift-up.png);}
.sort-bar div.sift-normal.sift-down:after{background-image: url(../images/sift-down.png);}
.sift-bar{height: 4.5rem;width: 100%;background-color: #fff;border-bottom: 1px solid #d8d8d8;padding-top: .7rem}
.sift-bar div{width: 20%;text-align: center;background-color: #f8f8f8;float:left;margin-left:4%;height: 3rem;border-radius: 5px;line-height: 2.9rem;color: #666;font-size: 1.4rem;border: 1px solid #f8f8f8;}
.sift-bar div .sort-down{width: 1rem;height: 1rem;display: inline-block;background-image: url(../images/sort-down.png);background-repeat: no-repeat;background-size: .7rem;background-position: 100% 50%;}
.sift-bar div .sift-icon{width: 1.3rem;height: 1rem;display: inline-block;background-image: url(../images/sift.png);background-repeat: no-repeat;background-size: 1rem;background-position: 100% 50%;}
.sift-bar div.on{background-color: #fff;border: 1px solid #00c000;color: #00c000;}
.sift-bar div.on .sift-icon{background-image: url(../images/sift-on.png);}
.sift-bar div.on .sort-down{background-image: url(../images/sort-up-on.png);}
