body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,p,blockquote,th,td,img {margin: 0;padding: 0;border: 0;outline: none;-webkit-tap-highlight-color: rgba(0, 0, 0, 0);-webkit-user-select: none;}
input,button,textarea {margin: 0;padding: 0;border: 0;outline: none;-webkit-tap-highlight-color: rgba(0, 0, 0, 0);}
input:-moz-placeholder {color: #a5a5a5;}::-webkit-input-placeholder {color: #a5a5a5;}
input {-webkit-appearance: none;-webkit-border-radius: 0;outline: none;}
ul,ol,li {list-style: none;}
a {color: #333;text-decoration: none; -webkit-touch-callout: none;cursor: pointer; outline: none;}
a:visited {color: #333;}
a:hover {color: #333;text-decoration: none;}
a:active {color: #333;}em,i {font-style: normal;}img {border: none;height: auto;max-width: 100%;}
.fl {
  float: left;
  display: inline;
}
.fr {
  float: right;
  display: inline;
}
.clear {
  height: 0px;
  line-height: 0px;
  clear: both;
}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
* + html .clearfix {
  overflow: hidden;
}
* html .clearfix {
  height: 1%;
}
.deal_main {
  width: 10rem;
  margin: 0 auto;
}
.deal_main .deal_top {
  padding-left: 0.4rem;
  width: 100%;
  background: #efeff4;
  font-size: 13px;
  height: 1.06666667rem;
  line-height: 1.06666667rem;
  color: #666666;
  box-sizing: border-box;
}
[data-dpr="2"] .deal_main .deal_top {
  font-size: 26px;
}
[data-dpr="2.5"] .deal_main .deal_top {
  font-size: 33px;
}
[data-dpr="2.75"] .deal_main .deal_top {
  font-size: 36px;
}
[data-dpr="3"] .deal_main .deal_top {
  font-size: 39px;
}
[data-dpr="4"] .deal_main .deal_top {
  font-size: 52px;
}
.deal_main .top_con {
  border-top: 1px solid #d8d8d8;
  border-bottom: 1px solid #d8d8d8;
}
.deal_main .top_con .con_tul {
  padding-left: 0.4rem;
  width: 100%;
  box-sizing: border-box;
}
.deal_main .heigh_20 {
  height: 0.26666667rem;
  width: 100%;
  background: #efeff4;
}
.con_tul li {
  color: #333333;
  padding: 0.37333333rem 0;
  border-bottom: 1px solid #d8d8d8;
}
.con_tul li em {
  float: left;
  font-size: 13px;
}
[data-dpr="2"] .con_tul li em {
  font-size: 26px;
}
[data-dpr="2.5"] .con_tul li em {
  font-size: 33px;
}
[data-dpr="2.75"] .con_tul li em {
  font-size: 36px;
}
[data-dpr="3"] .con_tul li em {
  font-size: 39px;
}
[data-dpr="4"] .con_tul li em {
  font-size: 52px;
}
.con_tul li span {
  font-size: 13px;
  float: left;
}
[data-dpr="2"] .con_tul li span {
  font-size: 26px;
}
[data-dpr="2.5"] .con_tul li span {
  font-size: 33px;
}
[data-dpr="2.75"] .con_tul li span {
  font-size: 36px;
}
[data-dpr="3"] .con_tul li span {
  font-size: 39px;
}
[data-dpr="4"] .con_tul li span {
  font-size: 52px;
}
.con_tul li .num {
  font-size: 13px;
}
[data-dpr="2"] .con_tul li .num {
  font-size: 26px;
}
[data-dpr="2.5"] .con_tul li .num {
  font-size: 33px;
}
[data-dpr="2.75"] .con_tul li .num {
  font-size: 36px;
}
[data-dpr="3"] .con_tul li .num {
  font-size: 39px;
}
[data-dpr="4"] .con_tul li .num {
  font-size: 52px;
}
.con_tul .last_no {
  border-bottom: none;
}
.con_tul .deal_img {
  float: left;
  width: 8.66666667rem;
  height: 4.13333333rem;
  margin-top: 0.26666667rem;
}
.con_tul .deal_img img {
  display: block;
  width: 100%;
  height: 100%;
}
